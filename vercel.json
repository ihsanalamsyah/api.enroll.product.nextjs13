{
    "version": 2,
    "builds":[
        {
            "src":"./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest":"/"
        }
    ],
    "functions": {
        "postinstall": {
            "runtime": "@now/node",
            "memory": 128
        }
    }
}